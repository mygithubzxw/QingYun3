{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(js|ico|css|png|jpg|jpeg|gif))",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/青云之志3算极限关卡.html"
        }
    ]
}